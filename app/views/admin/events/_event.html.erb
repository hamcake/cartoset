<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(event) -%>">

  <%= link_to event.title, edit_admin_event_path(event)%>

  <div id="icons">
  <%= link_to 'View', event_url(event), :class=> 'view', :target => "_blank" %>
  
  <%= link_to 'Delete', admin_event_path(event), :class=> 'cancel confirm-delete delete', :title => t('.delete'),
  :'data-confirm' => t('shared.admin.delete.message',
      :title => t('.delete')
    ),
      :'data-method' => :delete %>
  </div>
</li>
